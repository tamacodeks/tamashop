{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "arcanedev/log-viewer": "^4.5",
        "bacon/bacon-qr-code": "^2.0",
        "barryvdh/laravel-elfinder": "^0.3.12",
        "barryvdh/laravel-snappy": "0.4.8",
        "casinelli/currency": "dev-master",
        "doctrine/dbal": "^2.5",
        "ellipsesynergie/api-response": "^0.15.0",
        "encore/redis-manager": "^0.1.2",
        "fideloper/proxy": "^4.0",
        "flugger/laravel-responder": "^3.0",
        "geoip2/geoip2": "~2.0",
        "gloudemans/shoppingcart": "^2.6",
        "guzzlehttp/guzzle": "^6.3",
        "ixudra/curl": "^6.16",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "league/flysystem-sftp": "~1.0",
        "maknz/slack": "^1.7",
        "maknz/slack-laravel": "^1.0",
        "nexmo/laravel": "^1.1",
        "pragmarx/firewall": "^2.2",
        "pragmarx/google2fa-laravel": "^2.2",
        "pragmarx/google2fa-qrcode": "^3.0",
        "predis/predis": "^1.1",
        "spatie/array-to-xml": "^2.11",
        "spatie/laravel-backup": "^5.11",
        "spatie/laravel-fractal": "^5.3",
        "spatie/laravel-medialibrary": "^7.0.0",
        "stripe/stripe-php": "^13.13",
        "yajra/laravel-datatables-buttons": "^3.0",
        "yajra/laravel-datatables-html": "3.*",
        "yajra/laravel-datatables-oracle": "^8.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-view-xray": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "App/Library"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "settings.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
